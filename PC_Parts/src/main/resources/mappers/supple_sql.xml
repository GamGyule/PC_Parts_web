<!DOCTYPE mapper

  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Supple">
	<!-- <select id="getAll" resultType="com.gamgyul.gams.dto.CustomerDTO">
		select * from customer
	</select> -->

	    <select id="SuppleSelectAll" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product limit #{page}, 15
    </select>
    	
    	<select id="SuppleSelectName" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where name like '%'#{name}'%' limit #{page}, 15
    </select>
    	
    	<select id="SuppleSelectCo" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where co = #{co} limit #{page}, 15
    </select>
    	
    	<select id="SuppleSelectPriceD" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product order by price desc
    </select>
    
    <select id="SuppleSelectPriceA" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product order by price
    </select>
    
    	<select id="SuppleSelectPCntD" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product order by cnt desc
    </select>
    
    <select id="SuppleSelectPriceCnt" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product order by cnt
    </select>
    
    <select id="SuppleSelectCoPriceD" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where co = #{co} order by price desc
    </select>
    
    <select id="SuppleSelectCoPriceA" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where co = #{co} order by price
    </select>
    
    <select id="SuppleSelectNamePriceD" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where name like '%'#{name}'%' order by price desc 
    </select>
    
    <select id="SuppleSelectNamePriceA" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where name like '%'#{name}'%' order by price 
    </select>
    
        <select id="SuppleSelectCoCntD" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where co = #{co} order by cnt desc
    </select>
    
    <select id="SuppleSelectCoCntA" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where co = #{co} order by cnt
    </select>
    
        <select id="SuppleSelectNamePCntD" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where name like '%'#{name}'%' order by cnt desc
    </select>
    
    <select id="SuppleSelectNameCntA" resultType="com.pc.parts.dto.SuppleDTO">
        select * from product where name like '%'#{name}'%' order by cnt 
