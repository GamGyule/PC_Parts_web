<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper

  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="user">
		<insert id="notiInsert" parameterType="com.pc.parts.dto.NotiDTO">
	insert into values(
		#{idx},
		#{from_co},
		#{to_co},
		#{pid},
		#{cnt}]
		)
	
	</insert>
	    <select id="notiSelectAll" resultType="com.pc.parts.dto.NotiDTO">
        select * from noti
    </select>

	<select id="LoginCompany" resultType="com.pc.parts.dto.CompanyDTO">
		select * from company where id = #{idx} and pw = #{pw}
	</select>

</mapper>